<form action ="<%= users_url %>" method="Post">
  <input
    name="authenticity_token"
    value="<%= form_authenticity_token %>"
    type="hidden">

  <label> Username
    <input
      type="text"
      name="user[username]"
      value="<%= user.username %>">
  </label>
  <br>
  <label> Password
    <input
      type="password"
      name="user[password]"
      value="<%= user.password %>">
  </label>
  <br>
  <input type="submit">

</form>
